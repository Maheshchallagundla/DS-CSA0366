#include <stdio.h>       
int main()    
{    
    int arr[] = {1,2,-6,3,1,2,4,4,-4,-7,9,-6,-4};             
    int length = sizeof(arr)/sizeof(arr[0]);            
    printf("repeated characters in given array: \n");            
    for(int i = 0; i < length; i++) 
	{    
        for(int j = i + 1; j < length; j++)
		 {    
            if(arr[i] == arr[j])    
                printf("%d\n", arr[j]);    
        }    
    }    
    return 0;    
}  
